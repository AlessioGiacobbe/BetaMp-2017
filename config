00000 HALT
00001 LD A,n
00010 LD A,B
00011 LD A,C
00100 LD B,A
00101 LD C,A
00110 ADD A,B
00111 ADD A,C
01000 SUB A,C
01001 INC B
01010 DEC B
01011 JP addr
01100 IN A
01101 OUT A
01110 JP P, addr
01111 JP M, addr
10000 LD A, addr
10001 LD addr, A
10010 LD A,(B)
10100 JP Z, addr
10011 LD (B),A
